{
    "name": "lubem/gladepay",
    "description": "glade payment system for initiating and verifying bank transfer",
    "license": "MIT",
    "authors": [
        {
            "name": "Lubem Tser",
            "email": "enginlubem@ymail.com",
            "homepage": "http://enginlubem.com.ng"
        }
    ],
    "homepage": "https://github.com/lubem5612/glade-pay",
    "keywords": ["Laravel", "GladePay"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Lubem\\GladePay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lubem\\GladePay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lubem\\GladePay\\GladePayServiceProvider"
            ],
            "aliases": {
                "GladePay": "Lubem\\GladePay\\Facades\\GladePay"
            }
        }
    }
}
